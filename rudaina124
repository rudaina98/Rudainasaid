start
for user add prod
read price_prod, category_prod
IF (price_prod >=35 OR) then
user will get free express shopping and delivery
ELSE IF (price_prod>=50 OR)
user will get free delivery and descount 
ELSE
the user pay for customs and delivery
END IF 
IF category_producer ="shoes"
the user will receive a gift 
ELSE
The user will not receive a gift
END IF
END FOR 

